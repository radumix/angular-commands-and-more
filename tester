const { test, expect } = require('@playwright/test');

test('Test in incognito mode', async ({ browser }) => {
  // Create a new isolated context (incognito-like)
  const context = await browser.newContext({
    storageState: null, // Optional: Clear any stored session/cookies
    bypassCSP: true,    // Optional: If you want to disable Content Security Policy
  });

  // Open a page within the incognito context
  const page = await context.newPage();

  // Visit your URL
  await page.goto('https://example.com');

  // Add any interactions here
  await expect(page).toHaveTitle('Example Domain');

  // Close the context (and page) after the test
  await context.close();
});
