html inspect 

function findElement(xpath) {
    try {
        // Try to find the element using XPath
        let element = document.evaluate(xpath, document, null, XPathResult.FIRST_ORDERED_NODE_TYPE, null).singleNodeValue;
        
        if (element) {
            // Element found, return it
            return element;
        } else {
            // Element not found
            throw new Error("Element not found with XPath: " + xpath);
        }
    } catch (error) {
        // Handle the error (e.g., element not found)
        console.error(error.message);
        return null; // Or handle the case where the element is missing
    }
}

function checkElements() {
    // Try the primary XPath first
    let element = findElement("//primary_xpath_here");
    
    if (!element) {
        // If the primary element is not found, try the fallback XPath
        element = findElement("//fallback_xpath_here");
    }
    
    if (!element) {
        // If neither element is found, handle accordingly
        console.log("Both primary and fallback elements are missing.");
    } else {
        // Do something with the element if found
        console.log("Element found:", element);
    }
}

// Execute the function to check for elements
checkElements();


  

