html inspect 

// Correcting the contentObject structure
var contentObject = {
    "2 Role's Assigned": ["Alfreds Futterkiste", "Maria Anders"],
    "2 Role's Removed": ["Centro comercial Moctezuma", "Francisco Chang"]
};

// Start building the HTML table
var htmlTable = "<table border='1' style='border-collapse: collapse; width: 100%;'>";
htmlTable += "<tr>";

// Generate table headers
for (var header in contentObject) {
    htmlTable += "<th>" + header + "</th>";
}
htmlTable += "</tr>";

// Find the max number of rows
var maxRows = Math.max(
    contentObject["2 Role's Assigned"].length,
    contentObject["2 Role's Removed"].length
);

// Generate table rows
for (var i = 0; i < maxRows; i++) {
    htmlTable += "<tr>";
    for (var header in contentObject) {
        var value = contentObject[header][i] || ""; // Handle missing values
        htmlTable += "<td>" + value + "</td>";
    }
    htmlTable += "</tr>";
}

htmlTable += "</table>";

// Set the HTML table into the form field
$form.setValue('/my:myFields/my:emailContent', htmlTable);

