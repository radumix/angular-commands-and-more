html inspect 


  import { test, expect, chromium } from '@playwright/test';

test('block cookies during incognito test', async () => {
  const browser = await chromium.launch();
  const context = await browser.newContext();

  // Block cookies by intercepting network requests
  context.on('route', route => {
    if (route.request().url().includes('cookie')) {
      route.abort();  // Block cookies
    } else {
      route.continue();  // Continue normal request
    }
  });

  const page = await context.newPage();
  await page.goto('https://example.com');

  const cookies = await context.cookies();
  console.log('Cookies after blocking:', cookies);  // Should be empty or blocked

  await browser.close();
});




