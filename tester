// Locate the row that contains the "No records available" message
const noRecordsRowLocator = page.locator('table tbody tr.k-grid-norecords');

// Wait for the row to be visible or detached (in case it's removed when data loads)
const noRecordsVisible = await noRecordsRowLocator.isVisible();

if (noRecordsVisible) {
    // If the "No records available" message is visible, log it
    const noRecordsText = await noRecordsRowLocator.textContent();
    console.log('Message in the table:', noRecordsText.trim());
} else {
    // If the "No records available" message is NOT visible, check for data rows
    const dataRowsLocator = page.locator('table tbody tr');
    const rowCount = await dataRowsLocator.count();
    
    if (rowCount > 0) {
        console.log(`Table has ${rowCount} data rows.`);
        // Optionally, log or work with the first data row
        const firstDataRowText = await dataRowsLocator.nth(0).textContent();
        console.log('First data row text:', firstDataRowText.trim());
    } else {
        console.log('Table is empty, but no "No records available" message.');
    }
}
